<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_param_purchase_journal_by_supplier_report" model="ir.ui.view">
			<field name="name">view.param.purchase.journal.by.supplier.report</field>
			<field name="model">param.purchase.journal.by.supplier.report</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Purchase Journal By Supplier Report">
					<separator string="Purchase Journal By Supplier Report" colspan="4"/>
					<label colspan="4" nolabel="1" string="This menu prints Purchase Journal By Supplier Report"/>
					<newline/>
					<separator string="Date"  colspan="4"/>
					<field name="date_from" attrs="{'required':1}"/>
					<field name="date_to" attrs="{'required':1}"/>
					<separator string="Supplier" colspan="4"/>
					<field name="partner_code_from" options='{"quick_create": false}'
						domain="[('supplier','=',True)]" context="{'search_default_supplier':1,'default_supplier':1,'default_customer':0}"/>
					<field name="partner_code_to" options='{"quick_create": false}'
						domain="[('supplier','=',True)]" context="{'search_default_supplier':1,'default_supplier':1,'default_customer':0}"/>
					<separator string="Invoice No" colspan="4"/>
					<field name="inv_from" domain="[('type','in',['in_invoice','in_refund']), ('state','in',['open','paid'])]"/>
					<field name="inv_to" domain="[('type','in',['in_invoice','in_refund']), ('state','in',['open','paid'])]"/>
					<group col="2" colspan="4">
						<button icon='gtk-cancel' special="cancel" string="Cancel" />
						<button name="create_vat" string="Print" colspan="1" type="object" icon="gtk-ok"/>
					</group>
				</form>
			</field>
		</record>

		<record id="action_param_purchase_journal_by_supplier_report" model="ir.actions.act_window">
			<field name="name">Purchase Journal By Supplier Report</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">param.purchase.journal.by.supplier.report</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
			<field name="help">This menu print Purchase Journal By Supplier Report.</field>
		</record>

		<menuitem
			id="menu_param_purchase_journal_by_supplier_report"
			name="Purchase Journal By Supplier Report"
			action="action_param_purchase_journal_by_supplier_report"
			parent="menu_custom_account" sequence="2"/>
	</data>
</openerp>
